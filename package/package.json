{
  "name": "agent-oven",
  "version": "0.1.0",
  "description": "macOS-native job scheduler TUI for Docker containers",
  "type": "module",
  "main": "dist/core/index.js",
  "types": "dist/core/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./tui": {
      "types": "./dist/tui/index.d.ts",
      "import": "./dist/tui/index.js"
    }
  },
  "bin": {
    "agent-oven": "dist/cli.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "dist/**/*.js.map",
    "!dist/**/*.test.*",
    "!dist/**/__tests__/**",
    "images/",
    "scheduler.sh",
    "README.md",
    "LICENSE",
    "CLAUDE.md"
  ],
  "scripts": {
    "build": "tsc",
    "start": "tsx src/cli.tsx",
    "dev": "tsx watch src/cli.tsx",
    "init": "tsx src/cli.tsx init",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "scheduler",
    "docker",
    "tui",
    "cli",
    "macos",
    "colima"
  ],
  "author": "Fernando Rojas <fernando@fre-studios.com> (https://fre-studios.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FRE-Studios/Agent-Oven.git"
  },
  "homepage": "https://github.com/FRE-Studios/Agent-Oven#readme",
  "bugs": {
    "url": "https://github.com/FRE-Studios/Agent-Oven/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/FRE-Studios"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^14.0.3",
    "execa": "^9.5.2",
    "ink": "^5.1.0",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@vitest/coverage-v8": "^3.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
